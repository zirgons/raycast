function find_close()
	local wh=0
	local how=100
	for e in all(enemy) do
		if e.see==1 then
			if abs(x-e.x)+abs(y-e.y)<how then
				how = abs(x-e.x)+abs(y-e.y)
				wh=e
				
			end
		end
	end
	return wh
end

function empty(t)
	for i in all(t) do
		return false
	end
	return true
end

function screen_shake()
		local fade=0.95
  local offset_x=16-rnd(32)
  local offset_y=16-rnd(32)
  offset_x*=offset
  offset_y*=offset
  
  camera(offset_x,offset_y)
  offset*=fade
  if offset<0.05 then
    offset=0
  end
end
