--raycast

function raycast(nx,ny,trg)
	local speed=5

	for i=0,1,0.125/4 do
		//print(i,50,10,10)
			local c=speed
			local lf=30
			while true do
				lf-=1
				if lf==0 then
					break
				end
				local rx=sin(i)*c
				local ry=cos(i)*c
			 
				if fget(mget((nx+rx)/8,(ny+ry)/8),0) then
					break
				end
				for e in all(enemy)do
					if collision(nx+rx,ny+ry,1,1,e.x,e.y,12,12)then
						e.see=1
					end
				end
				c+=speed
		end
		
	end
	return false
end

