--player

function input()
	looku=0
	if btn(0) then
		if wall(x-1,y,w,h)==0 then
			x-=1;
			look=t
		end
	end
	if btn(1) then
		if wall(x+1,y,w,h)==0 then
			x+=1;
			look=f
			
		end
	end
	if btn(2) then
		if wall(x,y-1,w,h)==0 then
			y-=1;
			looku=2
		end
	end
	if btn(3) then
		if wall(x,y+1,w,h)==0 then
			y+=1;
			looku=1
		end
	end
	
	
	
	
	if(btn(4)and stime<=0)then

		stime=rtime
		
		offset=bl/26
		local ll = find_close()
		local t1=0
		local t2=0
		if ll==0 then
			local tmp=0
			if looku==1 then
				tmp=1
			elseif looku==2 then
				tmp=-1
			end
			if look then
				shoot(x-1,y+tmp,tsp,sprd)
				t1=x-1
				t2=y+tmp
			else
				shoot(x+1,y+tmp,tsp,sprd)
				t1=x+1
				t2=y+tmp
			end
		else
			shoot(ll.x,ll.y,tsp,sprd)
			t1=ll.x
			t2=ll.y
		end
		local angle=atan2(t1-x,t2-y)
		if wall(x-cos(angle)*bl,y-sin(angle)*bl,w,h)==0 then
			x-=cos(angle)*bl
			y-=sin(angle)*bl
		end
	end
	if btn(5) then
		if click==false then
			switchwep()
			//text=guns[1]
		end

		click=true
	else
		click=false
	end
end

