--collision

function collision(x1,y1,w1,l1,x2,y2,w2,l2)
	if x1 > x2+w2 or
	x2 > x1+w1 or
	y1 > y2+l2 or
	y2 > y1+l1 then
		return f
	end
	return t
end

function wall(nx,ny,nw,nh)
	
	if fget(mget(nx/8,ny/8),0) or fget(mget((nx+nw)/8,ny/8),0) then
		return 1
	end
	if fget(mget(nx/8,(ny+nh)/8),0) or fget(mget((nx+nw)/8,(ny+nh)/8),0) then		
		return 1
	end
	return 0
end

