--particles

function build_prt(cnt,nx,ny,nclr,ntime)
	for i=0,cnt do 
		add(particle,{
			x=nx,
			y=ny,
			dx=(rnd(20)-10)/10,
			dy=(rnd(20)-10)/10,
			lf=rnd(7)+ntime,
			speed=rnd(4),
			size=1,
			clr=nclr
		})
	end
	
end

function p_upd(p)
	p.x+=p.dx*p.speed
	p.y+=p.dy*p.speed
	p.lf-=1
	if p.lf<=0 then
		del(particle,p)
	end
end

function p_draw(p)
	circfill(p.x,p.y,p.size,p.clr)
	pset(p.x,p.y,p.clr+1)
end
