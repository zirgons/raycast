--shooting

function shoot(ndx,ndy,tsp,sprd)
		
	local angle = atan2(ndx-x,ndy-y)
	text=angle
	local ftime=0
	if guns[1]==2then
		ftime=7
	end
	
	for i=0,ftime do
		local angle2=angle+(rnd(sprd/10)-(sprd/10/2))

		add(bullet,{
			x=bulsetx,
			speed=tsp,
			y=bulsety,
			dx=cos(angle2),
			dy=sin(angle2+6),
			lifetime=40,
			frame=0
		})
	end
end
function b_draw(b)
	//spr((b.frame/2)+32,b.x, b.y,1,1)
	spr(23,b.x,b.y,5,5)
	
end

function b_update(b)
	b.x+= b.dx*b.speed
	b.y+= b.dy*b.speed
	b.lifetime-=1
	b.frame=flr(b.lifetime/5)
	
	if wall(b.x,b.y,5,5)==1 then
		build_prt(10,b.x,b.y,8,5)
		del(bullet,b)
	end
	
	if(b.lifetime==0)then
		build_prt(10,b.x,b.y,8,5)
		del(bullet,b)
	end
end
