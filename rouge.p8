--enemies

function addenemy(ex,ey,nsp)
	local nhp=0
	if nsp==0 then
		nhp=100
	elseif nsp==1 then
		nhp=3
	end
	
	local npx
	local npy
	local nph
	local npw

	if nsp==0 then
		npx=24
		npy=0
		npw=11
		nph=12
	elseif nsp==1 then
		npx=56
		npy=0
		npw=8
		nph=8
	end
	
	
	add(enemy,{
		x=ex,
		y=ey,
		hp=nhp,
		sp=nsp,
		spx=npx,
		spy=npy,
		spw=npw,
		sph=nph,
		tx=ex,
		ty=ey,
		see=0
	})
end

function e_update(e)
	for b in all(bullet)do
		if collision(e.x,e.y,12,12,b.x,b.y,4,4)then
			build_prt(10,b.x,b.y,8,5)
			e.x+=b.dx*b.speed*1
			e.y+=b.dy*b.speed*1
			del(bullet,b)
			e.hp-=2		
		end
	end	
	
	if e.sp==1 then
		if abs(e.tx-e.x)+abs(e.ty-e.y)<1 and e.see==0 then
			e.tx=e.x + (rnd(20)-10)*2
			e.ty=e.y + (rnd(20)-10)*2
		end
	elseif e.sp==0 then
		if e.see==1 then
			e.tx=x
			e.ty=y
		end
	end
	
	local angle = atan2(e.tx-e.x,e.ty-e.y)
	if wall(e.x+cos(angle)*0.3,e.y+sin(angle)*0.3,e.spw,e.sph)==1 then
		e.tx=e.x + (rnd(20)-10)*2
		e.ty=e.y + (rnd(20)-10)*2
	else
		e.x += cos(angle)*0.3
		e.y += sin(angle)*0.3
	end
	if e.hp<1 then
		build_prt(30,e.x,e.y,12,20)
		del(enemy,e)
	end
	e.see=0
end

function e_draw(e)
	sspr(e.spx,e.spy,e.spw,e.sph,e.x,e.y)
end
	 
