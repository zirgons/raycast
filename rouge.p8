--draw

function _draw()
	cls()
	screen_shake()
	map(0,0,0,0,128,32)
	foreach(bullet,b_draw)
	foreach(enemy,e_draw)
	foreach(particle,p_draw)
	
	print(text.." "..stat(7),50,10,6)
	sspr(40,0,13,13,x,y,13,13,look)
	
	
	print(hp.."|"..mxhp,7,6,2)
	local tmp=0
	if gunw==8 then
		tmp=0
	else
		tmp=-2
	end
	sspr(gunx,guny,gunw,gunh,92+tmp,7,gunw,gunh)
	if gun2w==8 then
		tmp=0
	else
		tmp=-2
	end
	sspr(gun2x,gun2y,gun2w,gun2h,108+tmp,7,gun2w,gun2h)
	
	
	
	local ll = find_close()	
	if	ll!=0 then 
		circ(ll.x+(ll.spw/2),ll.y+(ll.sph/2),5,7)
	end
	draw_gun()
	
	
	
end


function draw_gun()
	local ll = find_close()
	local angle
	if ll!=0 then
		angle=atan2(ll.x-x+6,ll.y-y+6)
	else
		local tmp=0
		if looku==1 then
			tmp=1	
		elseif looku==2 then
			tmp=-1
		end
		if look then
			angle=atan2(x-1-x,y+tmp-y)
		else
			angle=atan2(x+1-x,y+tmp-y)
		end
	end
	gunsetx=4+x+(cos(angle)*7)
	gunsety=4+y+(sin(angle)*7)
	bulsetx=4+x+(cos(angle)*8)
	bulsety=4+y+(sin(angle)*8)

	local gun_look=f
	if cos(angle)<0 then
		gun_look=t
	end
	local whichfr=0
	if sin(angle)>0.5 then
		whichfr=1 
	end
	if sin(angle)<-0.5 then
		whichfr=2
	end
	if sin(angle)>0.9 then
		whichfr=3
	end
	if sin(angle)<-0.9 then
		whichfr=4
	end
	sspr(gunx+(whichfr*gunw),guny,gunw,gunh,gunsetx,gunsety,gunw,gunh,gun_look)
end
